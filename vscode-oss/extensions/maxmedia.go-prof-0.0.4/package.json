{
	"name": "go-prof",
	"displayName": "Go Profiling",
	"description": "Go language profiling",
	"version": "0.0.4",
	"publisher": "MaxMedia",
	"author": "MaxMedia",
	"license": "MIT",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/MaxM65dia/vscode-go-prof"
	},
	"homepage": "https://maxmapps.com/",
	"engines": {
		"vscode": "^1.33.0"
	},
	"icon": "media/icon.png",
	"keywords": [
		"go",
		"pprof",
		"go profiling"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:go",
		"onCommand:goprof.profileCpuBenchmark",
		"onCommand:goprof.profileMemBenchmark"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "goprof.profileCpuBenchmark",
				"title": "Profile Cpu Benchmark"
			},
			{
				"command": "goprof.profileMemBenchmark",
				"title": "Profile Mem Benchmark"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "callGraph",
					"title": "CallGraph",
					"icon": "media/viewIcon.svg"
				}
			]
		},
		"views": {
			"callGraph": [
				{
					"id": "callGraph",
					"name": "CallGraph",
					"when": "callGraphViewEnabled"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"dist": "webpack --mode production",
		"compile": "tsc -p ./",
		"watch": "webpack --mode none --watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test",
		"unit": "yarn run compile && mocha --exclude \"out/**/extension.test.js\" --recursive \"out/**/*.test.js\""
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42",
		"@types/chai": "^4.1.7",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"mocha": "^6.1.3",
		"chai": "^4.2.0",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.0",
		"ts-loader": "^5.3.3"
	},
	"__metadata": {
		"id": "9054b43a-5670-4616-8a43-afdf1d0a49e8",
		"publisherId": "7da57783-7242-4fc2-8e4c-5adbb66ed028",
		"publisherDisplayName": "MaxMedia"
	}
}